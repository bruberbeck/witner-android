<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:endColor="@color/colorSecondaryLight" android:startColor="@color/colorSecondaryLight" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:endColor="@color/colorSecondary" android:startColor="@color/colorSecondary" />
        </shape>
    </item>
</selector>
